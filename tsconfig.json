{
  "compilerOptions": {                     
    "target": "ES2016",
    "module": "commonjs",
    "outDir": "./dist", 
    "rootDir": "./src",
    "strict": true,
    "baseUrl": "./",   
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "paths": {
      "#/*":["./src/*"]
    }
  }
}
